    "webpack-cli": "^3.2.0",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.18.0",
    "@blueprintjs/datetime": "^3.9.2",
    "@blueprintjs/select": "^3.10.0",
    "chokidar": "^3.0.2",
    "dexie": "^2.0.4",
    "fs-extra": "^8.1.0",
    "fsevents": "^2.1.3",
    "image-size": "^0.8.3",
    "mobx": "^5.13.0",
    "mobx-react-lite": "^1.4.1",
    "react": "^16.8.6",
