{
  "name": "allusion",
  "version": "0.0.1",
  "description": "A tool for managing your visual library",
  "main": "main.bundle.js",
  "scripts": {
    "development": "rimraf dist && webpack --watch --config ./webpack.dev.js --progress --colors",
    "production": "rimraf dist && webpack --config ./webpack.prod.js --progress --colors && ncp ./package.json dist/package.json",
    "start": "electron ./dist/main.bundle.js",
    "package": "rimraf release && npm run production && electron-packager ./dist Allusion --out=./release --asar",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest",
    "package:mac": "rimraf release && npm run production && electron-packager ./dist Allusion --out=./release --asar --icon=./src/renderer/resources/logo/allusion-logomark.icns"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RvanderLaan/VisualLibrary.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RvanderLaan/VisualLibrary/issues"
  },
  "homepage": "https://github.com/RvanderLaan/VisualLibrary#readme",
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-inlinesvg": "^0.8.0",
    "@types/react-window": "^1.8.0",
    "@types/uuid": "^3.4.4",
    "css-loader": "^2.1.0",
    "electron": "^5.0.1",
    "electron-packager": "^13.0.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-haste-map": "^24.7.1",
    "jest-resolve": "^24.7.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "react-inlinesvg": "^0.8.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.15.0",
    "@blueprintjs/select": "^3.8.0",
    "dexie": "^2.0.4",
    "fs-extra": "^7.0.1",
    "masonic": "^1.0.23",
    "mobx": "^5.9.0",
    "mobx-react-lite": "^1.1.1",
    "react": "^16.8.6",
    "react-dnd": "^7.3.2",
    "react-dnd-html5-backend": "^7.2.0",
    "react-dom": "^16.8.5",
    "react-window": "^1.8.1",
    "rimraf": "^2.6.3",
    "sourcemapped-stacktrace": "^1.1.9",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2"
  }
}
