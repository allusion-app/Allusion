{
  "name": "allusion",
  "productName": "Allusion",
  "version": "0.0.1",
  "description": "A tool for managing your visual library",
  "main": "main.bundle.js",
  "scripts": {
    "development": "rimraf dist && webpack --watch --config ./webpack.dev.js --progress --colors",
    "production": "rimraf dist && webpack --config ./webpack.prod.js --progress --colors && ncp ./package.json dist/package.json",
    "start": "electron ./dist/main.bundle.js",
    "package": "rimraf release && npm run production && electron-packager ./dist Allusion --out=./release --asar",
    "lint": "eslint src/**/*{ts,tsx} -f codeframe",
    "test": "jest",
    "package:mac": "rimraf release && npm run production && electron-packager ./dist Allusion --out=./release --asar --icon=./resources/logo/allusion-logomark.icns",
    "storybook": "start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RvanderLaan/VisualLibrary.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RvanderLaan/VisualLibrary/issues"
  },
  "homepage": "https://github.com/RvanderLaan/VisualLibrary#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/react": "^5.3.18",
    "@svgr/webpack": "^5.3.1",
    "@types/chrome": "^0.0.86",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.0",
    "@types/offscreencanvas": "^2019.6.0",
    "@types/react": "^16.9.34",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.6",
    "@types/react-transition-group": "^4.2.2",
    "@types/react-window": "^1.8.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.2",
    "electron": "^8.2.1",
    "electron-packager": "^14.2.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.3.0",
    "ncp": "^2.0.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.3.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.25.0",
    "@blueprintjs/datetime": "^3.16.0",
    "@blueprintjs/select": "^3.12.1",
    "chokidar": "^3.0.2",
    "dexie": "^2.0.4",
    "fs-extra": "^9.0.0",
    "image-size": "^0.8.3",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.6",
    "react": "^16.13.1",
    "react-color": "^2.18.0",
    "react-dom": "^16.13.1",
