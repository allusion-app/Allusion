{
  "name": "allusion",
  "productName": "Allusion",
  "version": "0.0.1",
  "description": "A tool for managing your visual library",
  "main": "main.bundle.js",
  "scripts": {
    "development": "rimraf dist && webpack --watch --config ./webpack.dev.js --progress --colors",
    "production": "rimraf dist && webpack --config ./webpack.prod.js --progress --colors && ncp ./package.json dist/package.json",
    "start": "electron ./dist/main.bundle.js",
    "package": "rimraf release && npm run production && electron-packager ./dist Allusion --out=./release --asar",
    "lint": "eslint src/**/*{ts,tsx} -f codeframe",
    "test": "jest",
    "package:mac": "rimraf release && npm run production && electron-packager ./dist Allusion --out=./release --asar --icon=./src/renderer/resources/logo/allusion-logomark.icns"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RvanderLaan/VisualLibrary.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RvanderLaan/VisualLibrary/issues"
  },
  "homepage": "https://github.com/RvanderLaan/VisualLibrary#readme",
  "devDependencies": {
    "@types/chrome": "^0.0.86",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.17",
    "@types/offscreencanvas": "^2019.6.0",
    "@types/react": "^16.9.1",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-inlinesvg": "^0.8.0",
    "@types/react-transition-group": "^4.2.2",
    "@types/react-window": "^1.8.1",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "css-loader": "^2.1.0",
    "electron": "^5.0.8",
    "electron-packager": "^14.0.3",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "ncp": "^2.0.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.18.2",
    "react-inlinesvg": "^0.8.4",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
